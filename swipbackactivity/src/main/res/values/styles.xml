<resources>

    <!-- SwipLayout Start -->
    <declare-styleable name="SwipeBackLayout">
        <attr name="swipEdgeSize" format="dimension" />
        <attr name="swipEdgeFlag" format="enum" />
        <attr name="swipShadowLeft" format="reference" />
        <attr name="swipShadowRight" format="reference" />
        <attr name="swipShadowBottom" format="reference" />
    </declare-styleable>

    <attr name="SwipeBackLayoutStyle" format="reference" />
    <attr name="swipBorderRadius" format="dimension" />
    <attr name="swipType">
        <enum name="swipCircle" value="0" />
        <enum name="swipRound" value="1" />
    </attr>
    <attr name="swipSrc" format="reference"></attr>
    <!-- SwipLayout End -->
</resources>
